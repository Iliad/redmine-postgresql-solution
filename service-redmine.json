{
    "kind": "Service",
    "metadata": {
        "labels": {
            "external": "true",
            "${NS_HASH}": "${REDMINE_DEPLOY_NAME}-${REDMINE_DEPLOY_HASH}"
        },
        "name": "${REDMINE_SVC_NAME}-${REDMINE_SVC_HASH}"
    },
    "spec": {
        "selector": {
            "${NS_HASH}": "${REDMINE_DEPLOY_NAME}-${REDMINE_DEPLOY_HASH}"
        },
        "ports": [
            {
                "protocol": "TCP",
                "targetPort": ${REDMINE_PORT}
            }
        ]
    }
}
