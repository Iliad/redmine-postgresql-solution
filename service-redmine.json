{
    "kind": "Service",
    "metadata": {
        "labels": {
            "app": "{{.REDMINE_IMAGE}}",
            "owner": "{{.OWNER}}"
        },
        "name": "{{.REDMINE_SVC_NAME}}-{{.REDMINE_SVC_HASH}}"
    },
    "spec": {
        "ports": [
            {
                "protocol": "TCP",
                "targetPort": {{.REDMINE_PORT}},
                "port": {{.REDMINE_PORT}},
                "name": "redmine"
            }
        ]
    }
}
